	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/martinholden-skillsoft/node-percipio-slackcommand.git"
	},
	"keywords": [
		"skillsoft",
		"percipio"
	],
	"author": "Martin Holden &lt;martin.holden@skillsoft.com&gt;",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/martinholden-skillsoft/node-percipio-slackcommand/issues"
	},
	"dependencies": {
		"axios": "^0.18.0",
		"body-parser": "^1.18.2",
		"debug": "^4.1.1",
		"express": "^4.16.2",
		"lodash": "^4.17.11",
		"moment": "^2.24.0",
		"qs": "^6.7.0",
		"search-query-parser": "^1.5.2",
		"slack-block-kit": "^0.9.3"
	},
	"devDependencies": {
		"auto-changelog": "^1.13.0",
		"babel-eslint": "^10.0.1",
		"eslint": "^5.3.0",
		"eslint-config-airbnb-base": "^13.1.0",
		"eslint-config-prettier": "^4.3.0",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-prettier": "^3.0.1",
		"nodemon": "^1.14.10",
		"nyc": "^14.1.1",
		"prettier": "^1.16.4",
		"rimraf": "^2.6.3"
	}
