	"description": "Express app providing slack slash command for Percipio Federated Search",
	"main": "start.js",
	"scripts": {
		"start": "node ./start.js",
		"watch": "nodemon ./start.js",
		"lint": "eslint . --ignore",
		"preversion": "npm run lint",
		"postversion": "git push && git push --tags && npm run changelog && git push ",
